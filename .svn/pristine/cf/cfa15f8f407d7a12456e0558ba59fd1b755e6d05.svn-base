.WidgetL1R2,.WidgetL2R1,.WidgetGrid4 {
    display: flex;
}
.WidgetL1R2 .L1R2-left, .WidgetL1R2 .L1R2-right, .WidgetL2R1 .L2R1-left, .WidgetL2R1 .L2R1-right, .WidgetGrid4 .grid4-col{
    flex: 1;
}
.WidgetL1R2 .L1R2-left,.WidgetL2R1 .L2R1-left {
    border-right: 1px solid #eee;
}
.WidgetL1R2 .L1R2-right img:last-child,.WidgetL2R1 .L2R1-left img:last-child, .WidgetGrid4 .grid4-col img:last-child{
    border-top: 1px solid #eee;
}
.WidgetGrid4 .grid4-col:first-child {
    border-right: 1px solid #eee;
}
.WidgetNavigation .navigation-ul {
    margin: 0;
    padding: 10px 0 0 0;
    display: flex;
    flex-wrap: wrap;
    list-style: none;

}
.WidgetNavigation .navigation-ul .navigation-item {
    flex: 1;
    width: 25%;
    min-width: 25%;
    max-width: 25%;
    min-height: 50px;
    text-align: center;
    font-size: 12px;
    float: left;
}
.WidgetNavigation .navigation-ul .navigation-item img {
    width: 40px;
    height: 40px;
    border-radius: 20px;
}
.WidgetNavigation .navigation-ul .navigation-item .anticon {
    font-size: 18px;
    background: #fff;
    padding: 10px;
    color: #eee;
    border-radius: 20px;
}
.WidgetNavigation .navigation-ul .navigation-item span {
    margin: 5px 0 10px 0;
    display: block;

}
.WidgetNavigation .navigation-ul .navigation-item span.sample {
    height: 16px;
    line-height: 16px;
    width: 50px;
    display: inline-block;
    background: #fefefe;
    border-radius: 4px;
}
.WidgetSeriesA .seriesa-ul {
    width: 100%;
    height: 60px;
    overflow: hidden;
    white-space:nowrap;
}
.WidgetSeriesA .seriesa-ul .seriesa-item {
    display: inline-block;
    float: left;
    margin-right: 10px;
}
.WidgetSeriesA .seriesa-ul .seriesa-item:last-child {
    margin-right: 0px;
}
.WidgetSeriesA .seriesa-ul .seriesa-item img{
    width: 60px;
    height: 60px;
}

.WidgetSeriesB .seriesb-banner img {
    width: 100%;
    max-height: 115px;
}
.WidgetSeriesB .seriesb-ul {
    margin: 0;
    padding: 0;
    display: flex;
    list-style: none;
    border-top: 1px solid #eee;
}
.WidgetSeriesB .seriesb-ul .seriesb-item{
    flex: 1;
    border-right: 1px solid #eee;
}
.WidgetSeriesB .seriesb-ul .seriesb-item img{
    width: 100%;
}
.WidgetSeriesB .seriesb-ul .seriesb-item:last-child{
    border-right: 0;
}